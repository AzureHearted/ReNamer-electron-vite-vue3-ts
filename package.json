{
  "name": "spuer-renamer",
  "version": "1.0.0",
  "description": "A batch renaming tool based on Electron and Vue development.",
  "main": "./out/main/index.js",
  "author": "AzureHearted",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "chcp 65001 && electron-vite preview",
    "dev": "chcp 65001 && electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "build:uncheck": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:uncheck:unpack": "electron-builder --dir",
    "build:uncheck:win": "npm run build:uncheck && electron-builder --win",
    "build:uncheck:mac": "npm run build:uncheck && electron-builder --mac",
    "build:uncheck:linux": "build:uncheck && electron-builder --linux",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "electron-updater": "^6.1.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.14.8",
    "@vicons/carbon": "^0.13.0",
    "@vicons/fluent": "^0.13.0",
    "@vicons/ionicons5": "^0.13.0",
    "@vicons/material": "^0.13.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vueuse/core": "^12.4.0",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-log": "^5.3.0",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "mime": "^4.0.6",
    "naive-ui": "^2.41.0",
    "pinyin-pro": "^3.26.0",
    "prettier": "^3.3.2",
    "sass": "^1.83.4",
    "typescript": "^5.5.2",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^5.3.1",
    "vite-plugin-lazy-import": "^1.0.7",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vue": "^3.4.30",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.0.22",
    "vxe-pc-ui": "^4.3.74",
    "vxe-table": "^4.10.5"
  }
}
